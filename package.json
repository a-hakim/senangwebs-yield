{
  "name": "senangwebs-yield",
  "version": "1.0.0",
  "description": "A lightweight, dependency-free JavaScript library for creating simple data visualizations (bar, pie, and line charts) using custom HTML attributes or a JavaScript API.",
  "main": "dist/swy.js",
  "unpkg": "dist/swy.min.js",
  "module": "dist/swy.esm.js",
  "types": "dist/swy.d.ts",
  "scripts": {
    "dev": "webpack serve --mode development --open",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write \"src/**/*.js\" \"examples/**/*.html\" \"docs/**/*.md\"",
    "test": "echo \"Test suite coming soon\"",
    "serve": "http-server examples -p 8080 -c-1"
  },
  "keywords": [
    "chart",
    "visualization",
    "bar-chart",
    "pie-chart",
    "line-chart",
    "javascript",
    "library",
    "lightweight",
    "no-dependencies",
    "data-visualization"
  ],
  "author": "a-hakim",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.8.1",
    "eslint": "^8.44.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "mini-css-extract-plugin": "^2.7.6",
    "prettier": "^3.0.0",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
